{
	"forceWifi": false,			// 
	"wifiSSID": "Logger.S",
	"wifiPSWD": "logger.s",
	"snake": true,
	"displayRows": 2,
	"noMoreEntries": true,
	"canSpeed": 500,
	"address": ["0xB8", "0x12", "0xF1", "0x03", "0x22", "0x40", "0x00", "0x3A"],
	"forceKwp": true,
	"ecuparam": [
		{
			"header": "Economy",
			"unit": "l/100km",
			"expr": "clip(100*x/(y+0.1))",
			"x": "Fuel usage",
			"y": "Speed"
		},
		{
			"header": "Econ avg 1",
			"unit": "l/100km",
			"expr": "avg(\"Econ avg 1\",x, y)",
			"x": "Economy",
			"y": "Speed",
			"avgLen": 20.0
		},
		{
			"header": "Econ avg 2",
			"unit": "l/100km",
			"expr": "avg(\"Econ avg 2\",x, y)",
			"x": "Economy",
			"y": "Speed",
			"avgLen": 20.0
		},
		{
			"header": "Trip 1",
			"unit": "km",
			"expr": "trip(\"Trip 1\",x)",
			"x": "Speed"
		},
		{
			"header": "Trip 2",
			"unit": "km",
			"expr": "trip(\"Trip 2\",x)",
			"x": "Speed"
		},
		{
			"header": "Fuel usage",
			"unit": "l/h",
			"expr": "(x/60)*(y/1000)*(250/16.6667)*3",  // Change 328i 250cc/min to 180cc/min for green injectors 320i/323i/325i, 280cc for 330i
			"x": "Engine Speed",
			"y": "Fuel Inj"
		},
		{
			"header": "Engine Speed",
			"unit": "RPM",
			"offset": 10,
			"length": 2,
            "mul": 0.25,
			"max": 6600,
			"dec": 0
		},
		{
			"header": "Engine Load",
			"unit": "mg/str",
			"offset": 23,
			"length": 2,
			"mul": 0.0015259,
			"dec": 1
		},
		{
			"header": "Mass Airflow",
			"unit": "kg/h",
			"offset": 21,
			"length": 2,
			"mul": 0.1,
			"dec": 1
		},
		{
			"header": "TPS",
			"unit": "%",
			"offset": 20,
			"mul": 0.39216,
			"max": 120,
